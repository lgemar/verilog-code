v0x9def2a8_0 .net "masked_data", 7 0, L_0x9def620; 1 drivers
v0x9def308_0 .net "rst", 0 0, v0x9def590_0; 1 drivers
E_0x9da5818 .event negedge, v0x9def0a0_0;
L_0x9def6d0 .reduce/or L_0x9def620;
    .scope S_0x9da13f8;
T_0 ;
    %wait E_0x9da5818;
    %load/v 8, v0x9def308_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_0.0, 8;
    %movi 8, 1, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x9def228_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x9def1c8_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x9def228_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x9def228_0, 0, 8;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x9da1bc0;
T_1 ;
    %delay 10000, 0;
    %load/v 8, v0x9def390_0, 1;
    %inv 8, 1;
    %set/v v0x9def390_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x9da1bc0;
T_2 ;
    %wait E_0x9da1478;
    %load/v 8, v0x9def540_0, 8;
    %mov 16, 0, 1;
    %movi 17, 40, 9;
    %mod 8, 17, 9;
    %cmpi/u 8, 0, 9;
    %inv 4, 1;
    %mov 8, 4, 1;
    %set/v v0x9def590_0, 8, 1;
    %load/v 8, v0x9def540_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %set/v v0x9def540_0, 8, 8;
    %jmp T_2;
    .thread T_2;
    .scope S_0x9da1bc0;
T_3 ;
    %wait E_0x9da5960;
    %load/v 8, v0x9def3f0_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %set/v v0x9def3f0_0, 8, 8;
    %jmp T_3;
    .thread T_3;
    .scope S_0x9da1bc0;
T_4 ;
    %vpi_call 2 42 "$dumpfile", "shift_out_test.vcd";
    %vpi_call 2 43 "$dumpvars", 1'sb0, S_0x9da1bc0;
    %set/v v0x9def540_0, 0, 8;
    %set/v v0x9def390_0, 0, 1;
    %set/v v0x9def3f0_0, 0, 8;
    %set/v v0x9def4d0_0, 0, 1;
    %set/v v0x9def590_0, 1, 1;
    %delay 100000, 0;
    %delay 50000, 0;
    %set/v v0x9def590_0, 0, 1;
    %delay 50000, 0;
    %set/v v0x9def590_0, 1, 1;
    %set/v v0x9def4d0_0, 1, 1;
    %movi 8, 1000, 11;
T_4.0 %cmp/s 0, 8, 11;
    %jmp/0xz T_4.1, 5;
