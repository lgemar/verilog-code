    .thread T_1, $push;
    .scope S_0x8487a38;
T_2 ;
    %set/v v0x84ce8d8_0, 0, 1;
    %set/v v0x84ceaf8_0, 0, 1;
    %set/v v0x84cea38_0, 0, 11;
T_2.0 ;
    %load/v 8, v0x84cea38_0, 11;
    %mov 19, 0, 2;
   %cmpi/u 8, 2045, 13;
    %jmp/0xz T_2.1, 5;
    %load/v 8, v0x84ce8d8_0, 1;
    %inv 8, 1;
    %set/v v0x84ce8d8_0, 8, 1;
    %load/v 8, v0x84cea38_0, 11;
    %mov 19, 0, 1;
    %movi 20, 4, 12;
    %mod 8, 20, 12;
    %cmpi/u 8, 0, 12;
    %jmp/0xz  T_2.2, 4;
    %set/v v0x84ce9c8_0, 1, 1;
    %jmp T_2.3;
T_2.2 ;
    %set/v v0x84ce9c8_0, 0, 1;
T_2.3 ;
    %vpi_call 2 36 "$display", "i = %d, out = %d\012", v0x84cea38_0, v0x84cea88_0;
    %load/v 8, v0x84cea38_0, 11;
    %mov 19, 0, 21;
    %addi 8, 1, 32;
    %set/v v0x84cea38_0, 8, 11;
    %jmp T_2.0;
T_2.1 ;
