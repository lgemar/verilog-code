    %load/vp0 8, v0x84ce6d0_0, 8;
    %set/v v0x84ce6d0_0, 8, 8;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x8487a38;
T_1 ;
    %wait E_0x84878a8;
    %load/v 8, v0x84cea88_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x84ce958_0, 0, 8;
