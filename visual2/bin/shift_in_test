:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x9579238 .scope module, "shift_in_test" "shift_in_test" 2 7;
 .timescale -9 -12;
P_0x957a82c .param/l "DATA_WIDTH" 2 11, +C4<01100>;
P_0x957a840 .param/l "WIDTH" 2 10, +C4<0100000>;
v0x95c7120_0 .var "clk", 0 0;
v0x95c71a0_0 .var "data_in", 0 0;
v0x95c7210_0 .net "data_out", 11 0, v0x95c6f78_0; 1 drivers
v0x95c7280_0 .var "ena", 0 0;
v0x95c72f0_0 .var "i", 31 0;
v0x95c7340_0 .var "rst", 0 0;
v0x95c73d0_0 .var/i "seed", 31 0;
S_0x957f8e0 .scope module, "uut" "shift_in" 2 22, 3 13, S_0x9579238;
 .timescale -9 -12;
v0x957d440_0 .net "clk", 0 0, v0x95c7120_0; 1 drivers
v0x95c6f18_0 .net "data_in", 0 0, v0x95c71a0_0; 1 drivers
v0x95c6f78_0 .var "data_out", 11 0;
v0x95c6fd8_0 .net "ena", 0 0, v0x95c7280_0; 1 drivers
v0x95c7040_0 .var "mask", 11 0;
v0x95c70a0_0 .net "rst", 0 0, v0x95c7340_0; 1 drivers
E_0x957d690 .event posedge, v0x957d440_0;
    .scope S_0x957f8e0;
T_0 ;
    %wait E_0x957d690;
    %load/v 8, v0x95c70a0_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 12, 0;
    %assign/v0 v0x95c6f78_0, 0, 0;
    %movi 8, 2048, 12;
    %ix/load 0, 12, 0;
    %assign/v0 v0x95c7040_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x95c6fd8_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x95c6f18_0, 1;
    %jmp/0xz  T_0.4, 8;
    %load/v 8, v0x95c6f78_0, 12;
    %load/v 20, v0x95c7040_0, 12;
    %or 8, 20, 12;
    %ix/load 0, 12, 0;
    %assign/v0 v0x95c6f78_0, 0, 8;
T_0.4 ;
    %load/v 8, v0x95c7040_0, 12;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 12;
    %ix/load 0, 12, 0;
    %assign/v0 v0x95c7040_0, 0, 8;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x9579238;
T_1 ;
    %delay 10000, 0;
    %load/v 8, v0x95c7120_0, 1;
    %inv 8, 1;
    %set/v v0x95c7120_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x9579238;
T_2 ;
    %set/v v0x95c73d0_0, 0, 32;
    %end;
    .thread T_2;
    .scope S_0x9579238;
T_3 ;
    %wait E_0x957d690;
    %vpi_func 2 33 "$random", 8, 32, v0x95c73d0_0;
    %set/v v0x95c71a0_0, 8, 1;
    %load/v 8, v0x95c72f0_0, 32;
    %mov 40, 0, 1;
    %addi 8, 1, 33;
    %set/v v0x95c72f0_0, 8, 32;
    %jmp T_3;
    .thread T_3;
    .scope S_0x9579238;
T_4 ;
    %wait E_0x957d690;
    %load/v 8, v0x95c72f0_0, 32;
    %movi 40, 40, 32;
    %mod 8, 40, 32;
    %cmpi/u 8, 0, 32;
    %inv 4, 1;
    %mov 8, 4, 1;
    %set/v v0x95c7340_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x9579238;
T_5 ;
    %vpi_call 2 42 "$dumpfile", "shift_in_test.vcd";
    %vpi_call 2 43 "$dumpvars", 1'sb0, S_0x9579238;
    %set/v v0x95c72f0_0, 0, 32;
    %set/v v0x95c7120_0, 0, 1;
    %set/v v0x95c71a0_0, 0, 1;
    %set/v v0x95c7280_0, 0, 1;
    %set/v v0x95c7340_0, 1, 1;
    %delay 100000, 0;
    %delay 50000, 0;
    %set/v v0x95c7340_0, 0, 1;
    %delay 50000, 0;
    %set/v v0x95c7340_0, 1, 1;
    %set/v v0x95c7280_0, 1, 1;
    %movi 8, 1000, 11;
T_5.0 %cmp/s 0, 8, 11;
    %jmp/0xz T_5.1, 5;
    %add 8, 1, 11;
    %wait E_0x957d690;
    %jmp T_5.0;
T_5.1 ;
    %vpi_call 2 66 "$finish";
    %end;
    .thread T_5;
