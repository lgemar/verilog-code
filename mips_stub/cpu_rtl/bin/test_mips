    .scope S_0x82a9e68;
T_2 ;
    %set/v v0x82dfb10_0, 0, 32;
T_2.0 ;
    %load/v 8, v0x82dfb10_0, 32;
   %cmpi/s 8, 4096, 32;
    %jmp/0xz T_2.1, 5;
    %ix/getv/s 3, v0x82dfb10_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x82dfc08, 0, 32;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x82dfb10_0, 32;
    %set/v v0x82dfb10_0, 8, 32;
    %jmp T_2.0;
T_2.1 ;
    %vpi_call 4 39 "$readmemh", P_0x82ac12c, v0x82dfc08;
    %end;
