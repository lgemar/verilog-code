    %jmp T_3.1;
T_3.0 ;
    %ix/getv 3, v0x82dfba8_0;
    %load/av 8, v0x82dfc08, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v0x82dfc98_0, 0, 8;
T_3.1 ;
