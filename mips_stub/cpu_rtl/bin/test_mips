S_0x82aaf48 .scope task, "dump" "dump" 4 52, 4 52, S_0x82a9e68;
 .timescale -9 -12;
TD_test_mips.MEMORY.dump ;
    %vpi_func 4 54 "$fopen", 8, 32, P_0x82ac140, "w";
    %set/v v0x82df858_0, 8, 32;
    %set/v v0x82dfb10_0, 0, 32;
T_0.0 ;
    %load/v 8, v0x82dfb10_0, 32;
   %cmpi/s 8, 4096, 32;
    %jmp/0xz T_0.1, 5;
    %vpi_call 4 56 "$fwrite", v0x82df858_0, "%h\012", &A<v0x82dfc08, v0x82dfb10_0 >;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x82dfb10_0, 32;
    %set/v v0x82dfb10_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 4 58 "$fclose", v0x82df858_0;
    %end;
    .scope S_0x82dfcf8;
T_1 ;
    %wait E_0x82a9f90;
    %load/v 8, v0x82f9e40_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0x82f9db0_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v0x82dfd78_0, 0, 0;
