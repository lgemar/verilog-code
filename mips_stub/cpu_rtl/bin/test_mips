    %set/v v0x82f9f00_0, 0, 1;
    %set/v v0x82fa170_0, 0, 1;
    %movi 8, 5, 4;
T_4.0 %cmp/s 0, 8, 4;
    %jmp/0xz T_4.1, 5;
    %add 8, 1, 4;
    %wait E_0x82a9f90;
    %jmp T_4.0;
T_4.1 ;
    %set/v v0x82fa170_0, 1, 1;
    %movi 8, 100, 8;
T_4.2 %cmp/s 0, 8, 8;
    %jmp/0xz T_4.3, 5;
    %add 8, 1, 8;
    %wait E_0x82a9f90;
    %jmp T_4.2;
T_4.3 ;
    %fork TD_test_mips.MEMORY.dump, S_0x82aaf48;
