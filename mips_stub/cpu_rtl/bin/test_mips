    .scope S_0x82a9e68;
T_3 ;
    %wait E_0x82a9f90;
    %load/v 8, v0x82df7f8_0, 1;
    %jmp/0xz  T_3.0, 8;
    %load/v 8, v0x82df790_0, 32;
    %ix/getv 3, v0x82dfba8_0;
    %jmp/1 t_1, 4;
